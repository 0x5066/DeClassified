    <text id="status" text="Playback Status: " x="-2" w="380" font="wasabi.font.default" align="left" valign="top"/>
    <text id="channel" text="Channel: " x="-2" y="7" w="380" font="wasabi.font.default" align="left" valign="top"/>
    <text id="kbps" text="kbps: " x="-2" y="14" w="380" font="wasabi.font.default" align="left" valign="top"/> 
    <text id="khz" text="khz: " x="-2" y="21" w="380" font="wasabi.font.default" align="left" valign="top"/>
    <text id="displayvolume" text="Volume: " x="-2" y="28" w="380" font="wasabi.font.default" align="left" valign="top"/>
    <text id="freqbands" text="Frequency Bands: " x="-2" y="35" w="380" font="wasabi.font.default" align="left" valign="top"/>
    <text id="timer" text="Time: " x="-2" y="42" font="wasabi.font.default" align="left" valign="top"/>
    <text id="isthiswacup" text="Is this WACUP?: " x="-2" y="49" font="wasabi.font.default" align="left" valign="top"/>

    <text text="DECLASSIFIED" x="0" y="53" w="380" font="Winamp" align="center" fontsize="43" valign="top"/>
