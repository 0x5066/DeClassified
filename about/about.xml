</groupdef>

<groupdef id="skin.about.group" w="0" h="0" relatw="1" relath="1" rectrgn="1">

    <rect w="380" h="321" color="x52" filled="1"/> <!-- using the window background pixel/color from classic-genex, use a different color if it doesnt look good -->
    <group id="aboutplayer" x="52" y="100"/>

    <rect y="313" w="380" h="11" color="0,0,0" filled="1"/>
    <Songticker
		id="songticker" ticker="1"
		x="0" y="314"
		w="380" h="9"
		font="wasabi.font.default" altfont="arial" fontsize="13"
		altvalign="center"
	/>
    <group id="cooldebuginfo" y="-55" alpha="0"/>

<script file="about/quakeconsole.maki"/>

</groupdef>