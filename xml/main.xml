<groupdef id="sliders">

	<AnimatedLayer id="volume" w="68" h="15" image="volume" start="0" end="27" frameheight="15" ghost="1"/>
	<Slider id="volumewa2" action="volume" orientation="horizontal" x="-3" y="-1" w="68" h="15" thumb="volbtn" downThumb="volbtnd"/>
	<script file="scripts/volume.maki"/>
	<AnimatedLayer id="balance" x="70" w="38" h="15" image="balance" start="0" end="27" frameheight="15" ghost="1"/>
	<Slider id="balancewa2" action="PAN" orientation="horizontal" x="70" y="-1" w="38" h="15" thumb="panbtn" downThumb="panbtnd"/>
	<script file="scripts/balance.maki"/>
</groupdef>

<groupdef id="waclassicvis">
	<vis id="wa.vis" y="0" w="72" h="16" fps="60"/>
	<layer id="main.vis.trigger" w="72" h="16"/>
	<script file="scripts/wa2visualizer.maki"/>
</groupdef>

<groupdef id="shademain">

    <layer id="blah" dblClickAction="SWITCH;normal" x="0" y="0" h="14" w="275" image="wa2.player.shade.enabled" inactiveimage="wa2.player.shade.disabled" move="1"/>

    <text id="timer" display="time" x="126" y="2" w="31" h="10" font="wasabi.font.default" fontsize="8" align="right"/>
	<layer id="TimerTrigger" x="126" y="2" w="31" h="10"/>

	<script file="scripts/wa2songtimer.maki"/>

    <button id="Previous" action="PREV" x="166" y="3" tooltip="Previous"/>
    <button id="Play" action="PLAY" x="176" y="3" tooltip="Play"/>
    <button id="Pause" action="PAUSE" x="186" y="3" tooltip="Pause"/>
    <button id="Stop" action="STOP" x="196" y="3" tooltip="Stop"/>
    <button id="Next" action="NEXT" x="206" y="3" tooltip="Next"/>

    <button id="Eject" action="EJECT" x="216" y="3" tooltip="Eject"/>

    <vis id="wa.vis" x="79" y="5" w="38" h="5" fps="60"/>
	<layer id="main.vis.trigger" x="79" y="5" w="38" h="5"/>

    <button id="winshade" inactivealpha="0" tooltip="Toggle Windowshade mode" action="SWITCH" param="normal" x="254" y="3" image="wa2.player.title.shade" downImage="wa2.player.title.shade.pressed"/>
    <button id="Close" inactivealpha="0" action="close" x="264" y="3" image="wa2.player.title.quit" downImage="wa2.player.title.quit.pressed" tooltip="Close" />
    <button id="Minimize" inactivealpha="0" action="MINIMIZE" x="244" y="3" image="wa2.player.title.min" downImage="wa2.player.title.min.pressed" tooltip="Minimize Winamp" />
    <button id="sysbutton" inactivealpha="0" action="SYSMENU" x="6" y="3" image="wa2.player.shade.menu" downImage="wa2.player.shade.menu.pressed" />
    <slider id="seeker" inactivealpha="0" action="seek" x="227" y="4" h="7" w="15" thumb="player.winshade.seek" downthumb="player.winshade.seek" />

<script file="scripts/wa2visualizer.maki"/>

</groupdef>

<container id="main" name="Main Window" default_x="0" default_y="0" default_visible="1" minimum_w="275" minimum_h="116" maximum_w="275" maximum_h="116">
	<layout id="normal" background="wa.main" droptarget="pldr">
		<group id="mainwindow"/>
	</layout>
	<layout id="shade" background="wa2.player.shade.enabled" inactive="wa2.player.shade.disabled" minimum_h="14" minimum_w="275">
		<group id="shademain" x="0" y="0" sysregion="1"/>
	</layout>
</container>