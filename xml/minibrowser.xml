<container id="minibrowser" name="Minibrowser" default_x="0" default_y="232" default_visible="0">
	<layout id="normal" background="wasabi.frame.basetexture" minimum_h="116" minimum_w="275" default_w="350" default_h="348">
		<layer id="minibrowser.topleft" image="mb.topleft.active" x="0" y="0" inactiveimage="mb.topleft.inactive" regionop="1" cursor="cur.move"/>
		<layer id="minibrowser.stretchybit" image="mb.top.stretchybit.active" x="25" y="0" w="-50" relatw="1" inactiveimage="mb.top.stretchybit.inactive" regionop="1" cursor="cur.move"/>
        <layer id="minibrowser.center" x="-100" y="0" image="mb.top.center.active" inactiveimage="mb.top.center.inactive" cursor="cur.move"/>
        <script file="scripts/_wa2-easylayout.maki" param="minibrowser.center;0;0;1;0;0;0"/>
		<layer id="minibrowser.topright" image="mb.topright.active" x="-25" y="0" relatx="1" inactiveimage="mb.topright.inactive" regionop="1" cursor="cur.move"/>
		<layer id="minibrowser.left" tile="1" image="mb.left" x="0" y="20" h="-58" relath="1" regionop="1" cursor="cur.normal"/>
		<layer id="minibrowser.right" tile="1" image="mb.right" x="-8" y="20" h="-58" relatx="1" relath="1" regionop="1" cursor="cur.normal"/>
		<layer id="minibrowser.bottomleft" image="mb.bottomleft" x="0" y="-38" relaty="1" regionop="1" cursor="cur.normal"/>
		<layer id="minibrowser.bottom.stretchybit" image="mb.bottom.stretchybit" x="125" y="-38" w="-250" relaty="1" relatw="1" regionop="1" cursor="cur.normal"/>
		<layer id="minibrowser.bottomright" image="mb.bottomright" x="-125" y="-38" relatx="1" relaty="1" regionop="1" cursor="cur.normal"/>
		<layer x="-19" y="-19" w="19" h="19" relatx="1" relaty="1" resize="bottomright" move="0" cursor="cur.resize"/>

		<winamp:browser fitparent="1"
		x="10" y="20" w="-19" h="-29" relatw="1" relath="1"
		home="https://getwacup.com/browser.html"
		wantfocus="0" mainmb="0"
		/>

		<!-- Close Button -->
		<button x="-11" y="3" relatx="1" relaty="0" action="Close" rectrgn="1" image="video.close" downImage="video.closep" tooltip="Close" inactiveAlpha="0" cursor="cur.close"/>
	</layout>
</container>